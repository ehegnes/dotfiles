! Xft Settings --------------------------------------------------------
Xft.dpi:        96
Xft.lcdfilter:  lcddefault
Xft.antialias:  true
Xft.rgba:       rgba
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.font:       xft:DejaVu Sans Mono-12

!-*- Perl extensions -*-
URxvt.perl-ext-common:default,clipboard,matcher,keyboard-select,font-size
URxvt.keysym.C-Up:    perl:font-size:increase
URxvt.keysym.C-Down:  perl:font-size:decrease
URxvt.keysym.C-S-Up:  perl:font-size:incglobal
URxvt.keysym.C-S-Down:perl:font-size:decglobal
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.M-u:     perl:url-select:select_next
URxvt.url-launcher:   /usr/bin/google-chrome-unstable
URxvt.underlineURLs:  True
URxvt.matcher.button: 1
URxvt.keysym.C-Escape:perl:keyboard-select:activate
URxvt.keysym.C-C:     perl:clipboard:copy
URxvt.keysym.C-V:     perl:clipboard:paste
URxvt.keysym.C-A-V:   perl:clipboard:paste_escaped
!URxvt.copyCommand:   xsel -ib
!URxvt.pasteCommand:  xsel -ob

! Tango Colorscheme
!*background: #2e3436
!URxvt*background: [80]#1c2022
URxvt*background: [70]#000000
!*foreground: #e6e6e6
*foreground: #eeeeec
! black
!*color0:  #2e3436
*color0:  #1c2022
*color8:  #555753
! red
*color1:  #a40000
*color9:  #ef2929
! green
*color2:  #4e9a06
*color10: #8ae234
! yellow
*color3:  #c4a000
*color11: #fce94f
! blue
*color4:  #3465a4
*color12: #729fcf
! purple
*color5:  #75507b
*color13: #ad7fa8
! orange (replaces cyan)
*color6:  #ce5c00
*color14: #fcaf3e
! white
*color7:  #babdb9
*color15: #eeeeec

